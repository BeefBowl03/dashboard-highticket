{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/generate-domains-simple.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/debug",
      "dest": "/api/debug.js"
    },
    {
      "src": "/api/generate-domains",
      "dest": "/api/generate-domains-simple.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
