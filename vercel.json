{
  "version": 2,
  "buildCommand": "cd dashboard-app && npm run build",
  "outputDirectory": "dashboard-app/build",
  "functions": {
    "dashboard-backend/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dashboard-backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard-app/build/$1"
    }
  ]
}
